            </form>
            <div class="team-comments col-lg-6 col-sm-6 col-xs-6 pull-right nopadri">
                <h3>Comments</h3>
                <ul id="team-comments">
                </ul>
                <form role="form" action="{{ urlFor('submit.comment') }}" method="post" id="comment-post">
                    <div class="form-group">
                        <textarea type="text" class="form-control" placeholder="Enter comment" name="comment"></textarea>
                    </div>
                    <div class="form-group">
                        <button class="btn btn-info {% if isMobile %}btn-sm{% endif %}" type="submit">Submit</button>
                    </div>
                    <input type="hidden" hidden="hidden" value="{{ csrf_token }}" name="{{ csrf_key }}">
                </form>
            </div>
            <div id="team-photo" class="col-lg-12">
                <button type="button" class="btn btn-info btn-sm" id="view-photo">View Photos</button>
                <button type="button" class="btn btn-info btn-sm" id="hide-photo">Hide Photos</button>
                <button type="button" class="btn btn-info btn-sm" id="photo-button">Submit Photos</button>
            </div>
        </div>
        <div class="col-sm-12 col-lg-6 col-xs-12 pull-left nodisplay" id="photo-gallery" data-blue-photos="{{ urlFor('team.get.chphotos') }}">
            <div class="col-lg-6 col-sm-6 col-xs-6" id="view-team-photos" data-run="0">
                <h3>Team Photos</h3>
            </div>
            <div class="col-lg-6 col-sm-6 col-xs-6" id="view-blue-photos">
                <h3>Blue Alliance Photos</h3>
            </div>
        </div>
        <div class="loader">
            <img class="loader-img" src="img/loader.gif">
        </div>
    </div>
</div>
